<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/carspeedlistener_tv_lvnum"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="序号" />

    <TextView
        android:id="@+id/carspeedlistener_tv_lvcarid"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="车号" />

    <TextView
        android:id="@+id/carspeedlistener_tv_lvcarspeed"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="速度" />

    <TextView
        android:id="@+id/carspeedlistener_tv_lvoverspeeddatetime"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="超速时间" />

</LinearLayout>